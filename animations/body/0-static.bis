#attribute name 静止状态
#import interp.bis

void main() {
  g_arm_l1 = 0.0;
  g_arm_r1 = 0.0;

  g_arm_l3 = 0.0;
  g_arm_r3 = 0.0;

  int i;
  for (i = 1; i <= 25; i++) {
    float dx = interp(1.0, 25.0, float(i));
    g_arm_l1 = dx;
    g_arm_r1 = dx;
    g_arm_l3 = dx;
    g_arm_r3 = -dx;
    yield;
  }
}
